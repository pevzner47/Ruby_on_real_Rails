<h1>Create new test</h1>

<form action="/tests" method="post">

  <input name="authenticity_token" value="<%= form_authenticity_token =%>" type="hidden">

  <p>
    <label for="test-title">Title:</label>
    <input type="text" name="test[title]" id="test_title">
  </p><br>

  <p>
    <label for="test-level">Level:</label>
    <input type="text" name="test[level]" id="test_level">
  </p><br>

  <p>
    <label for="test-category">Category_id:</label>
    <input type="text" name="test[category_id]" id="test-category">
  </p><br>

  <p>
    <label for="test-author_id">Author_id:</label>
    <input type="text" name="test[author_id]" id="test-author_id">
  </p><br>

  <input type="submit" value="Create">
</form>
